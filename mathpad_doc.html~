$(document).ready(
function(){
$(".exercice").each(function(i){
j=i+1;
$(this).css("padding","2px");
$(this).css("background-color","#f0f0f0");
$(this).prepend("<span style='font-weight:bold;'>Exercice " + j+"</span>");});
$(".exemple").each(function(i){
j=i+1;
$(this).css("padding","2px");
$(this).css("background-color","#f0f0f0");
$(this).prepend("<span style='font-weight:bold;'>Exemple " + j+"</span>");});
$(".def").each(function(i){
j=i+1;           
$(this).css("border","solid 1px black");
$(this).css("background-color","#f0f0f0");
$(this).css("padding","5px");
$(this).prepend("<span style='font-weight:bold;'>Définition " + j+"</span>");});
$(".prop").each(function(i){
j=i+1;
$(this).css("background-color","#f0f0f0");           
$(this).css("border","solid 1px black");
$(this).css("padding","5px");
$(this).prepend("<span style='font-weight:bold;'>Propriété " + j+"</span>");});
$(".theo").each(function(i){
j=i+1;
$(this).css("background-color","#f0f0f0");           
$(this).css("border","solid 1px black");
$(this).css("padding","5px");
$(this).prepend("<span style='font-weight:bold;'>Théorème " + j+"</span>");});
$(".rem").each(function(i){
j=i+1;
$(this).css("background-color","#f0f0f0");
$(this).prepend("<span style='font-weight:bold;'>Remarque " + j+"</span>");});
$(".paragraphe").each(function(i){
j=i+1;
$(this).css("background-color","#c0c0c0");
$(this).css("color","#fff");
$(this).css("padding","3px");
$(this).css("border-radius","10px");
$(this).prepend("<span style='font-weight:bold;color:#fff;background-color:#c0c0c0;border-radius:10px;padding:3px;'>"+j+" - </span>");});
$(".pause").each(function(i){
var cont = $(this).html();
$(this).html('');
$(this).prepend("<div id='trou_"+i+"' class='trou'>"+ cont +"</div>");
});
$(".corr").each(function(i){
var cont = $(this).html();
$(this).html('');
$(this).prepend("<button onclick='corrige("+i+")'>Correction</button><div id='correction_"+i+"' class='correction'>"+ cont +"<button style='display:block;' onclick='decorrige("+i+")'>&#10005;</button></div>");
});
}
);

var trou = 0;
function retourne() {
	if (trou>0) {trou--;	}
	var nom = "trou_"+trou;
	document.getElementById(nom).style.visibility = "hidden";	
	}
function avance() {
	var nom = "trou_"+trou;
	document.getElementById(nom).style.visibility = "visible";
	trou++;
	}
function corrige(n) {
	
	var nom = "correction_"+n;
	document.getElementById(nom).style.display = "block";
}
function decorrige(n) {
	var nom = "correction_"+n;
	document.getElementById(nom).style.display = "none";
}
</script>
